{
    "name": "FRC Qrscout 4063 2024",
    "short_name": "QRScout 4063",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#ff0000",
    "theme_color": "#ffffff",
    "icons": [
        {
            "src": "/icon-192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "/icon-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ],
    "serviceworker": {
        "src": "/service-worker.js",
        "scope": "/"
      },
      "caching": {
        "caches": ["app-shell", "images"],
        "app-shell": [
          "/",
          "/index.html",
          "/indexCascadingStylingSheets.css",
          "/indexJavaScript.js",
          "/QR-CodeGen.js"
        ],
        "images": [
          "/icon-192.png",
          "/icon-512.png"
        ]
      }
}
